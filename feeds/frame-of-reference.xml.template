<?xml version="1.0" encoding="utf-8"?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Frame of Reference</name>
  <summary>OmegaEngine sample game</summary>
  <feed-for interface="https://omegaengine.de/feeds/frame-of-reference.xml" />
  <group arch="Windows-i486">
    <requires interface="https://omegaengine.de/feeds/directx.xml" />
    <requires interface="https://omegaengine.de/feeds/msvcrt.xml" version="10..!11" />
    <command name="run" path="FrameOfReference.exe" />
    <command name="editor" path="FrameOfReference.Editor.exe" />
    <implementation version="{version}" local-path="../artifacts/Release/net472/publish">
      <requires interface="https://omegaengine.de/feeds/frame-of-reference-content.xml" version="{version}" />
      <requires interface="https://omegaengine.de/feeds/frame-of-reference-extra.xml" version="1.2..!1.3" />
      <manifest-digest />
      <archive href="frame-of-reference-{version}.tar.zst" type="application/x-zstd-compressed-tar" />
    </implementation>
  </group>
</interface>
